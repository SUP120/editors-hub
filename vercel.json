{
  "version": 2,
  "buildCommand": "CI=false npm run build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1",
    "NEXT_IGNORE_ESLINT": "true",
    "NEXT_IGNORE_REACT_HOOKS": "true",
    "TS_NODE_TRANSPILE_ONLY": "true",
    "CI": "false"
  }
} 